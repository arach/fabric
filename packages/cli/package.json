{
  "name": "fabric-ai",
  "version": "0.1.0",
  "description": "CLI for Fabric - Ambient compute for Claude Code agents",
  "type": "module",
  "bin": {
    "fabric": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build src/cli.ts --outdir dist --target node --external '@arach/*' --external '@daytonaio/*' --external '@e2b/*'",
    "dev": "bun run src/cli.ts",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "fabric-ai-core": "^0.1.0",
    "fabric-ai-daytona": "^0.1.0",
    "fabric-ai-e2b": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "fabric",
    "cli",
    "claude",
    "ai",
    "sandbox",
    "daytona",
    "e2b",
    "agents"
  ],
  "author": "Arach Tchoupani",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arach/fabric.git",
    "directory": "packages/cli"
  },
  "homepage": "https://fabric.arach.dev",
  "engines": {
    "node": ">=18"
  }
